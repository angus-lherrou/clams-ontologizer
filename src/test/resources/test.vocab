vocab="http//vocab.lappsgrid.org"
isocat="http://www.isocat.org/datcat"
schema="http://schema.org"

script {
    Thing "The most generic specification." {
        sameAs "$schema/Thing"
        uri "$vocab/Thing"
        properties {
            id String, "Everything can have an ID."
            alternateName String, "An alias for the item"
        }
    }
    Annotation "Describe Annotations here." {
        sameAs "$schema/Annotation"
        uri "$vocab/Annotation"
        properties {
            start Integer, "the starting offset of the annotation."
            end Integer, "the ending offset of the annotation."
        }
    }

}